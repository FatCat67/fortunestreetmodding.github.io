          <div class="btn-group" role="group" id="tables-group" aria-label="Basic example">
            <input type="radio" class="btn-check" name="gametype" id="gametype_csmm" value="csmm" autocomplete="off" onclick="simulator.set_csmm()" checked />
            <label class="btn btn-outline-primary" for="gametype_csmm">CSMM</label>
            <input type="radio" class="btn-check" name="gametype" id="gametype_vanilla" value="vanilla" autocomplete="off" onclick="simulator.set_vanilla()" />
            <label class="btn btn-outline-primary" for="gametype_vanilla">Vanilla</label>
          </div>
        </div>
      </div>
