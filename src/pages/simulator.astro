---
import Layout from '~/layouts/layout.astro';
import './simulator.css';
---
<script>
  import * as t from "./simulator";
  declare global { var simulator: typeof t; }
  globalThis.simulator = t;
  // run setup() function to init the simulator.js
  simulator.setup()
</script>
<Layout title="District Simulator" heading="District Simulator">
